extends layout

block content
	h1 Welcome, USER
	div(class="container")
		each user in users
			div(class="row")
				img(class="img-circle col-md-2" src="http://www.openingthebook.com/media/427156/image-1_large_thumb.jpg")
				div(class="col-md-6")
					h1 #{user.name}, #{user.school}
					h2 USER_%
					- var n = 0
					while n < 9
						h3= user.skills[n++]


	
